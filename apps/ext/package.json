{
    "name": "@eye-note/ext",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"vite build --watch --mode development\" \"cross-env CONTENT_SCRIPT=1 vite build --watch --mode development\" \"wait-on dist/assets/icon.svg && nodemon --delay 2.5 --ignore dist/cursors/ --ignore dist/icons/ --watch dist --exec 'node scripts/prepare-extension.js'\"",
        "build": "rm -rf dist && tsc && vite build && cross-env CONTENT_SCRIPT=1 vite build && wait-on dist/assets/icon.svg && node scripts/prepare-extension.js",
        "preview": "vite preview",
        "clean": "rm -rf dist extension node_modules"
    },
    "dependencies": {
        "@eye-note/auth": "workspace:*",
        "@eye-note/backend-health": "workspace:*",
        "@eye-note/groups": "workspace:*",
        "@eye-note/definitions": "workspace:*",
        "@eye-note/shortcuts": "workspace:*",
        "@eye-note/page-identity": "workspace:*",
        "@eye-note/ui": "workspace:*",
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-dialog": "^1.1.6",
        "@radix-ui/react-label": "^2.1.2",
        "@radix-ui/react-slot": "^1.1.2",
        "@radix-ui/react-switch": "^1.1.3",
        "@radix-ui/react-tabs": "^1.1.13",
        "@radix-ui/react-toast": "^1.2.6",
        "html2canvas": "^1.4.1",
        "next-themes": "^0.4.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "socket.io-client": "^4.7.5",
