{
    "name": "eye-note",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
    },
    "packageManager": "pnpm@8.15.1",
    "devDependencies": {
        "prettier": "^3.2.5"
    }
}
