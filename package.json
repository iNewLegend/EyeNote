{
    "name": "eye-note",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "pnpm --filter @eye-note/frontend dev",
        "build": "pnpm --filter @eye-note/frontend build",
        "build:extension": "pnpm --filter @eye-note/frontend build:extension",
        "watch": "pnpm --filter @eye-note/frontend dev:extension",
        "watch:frontend": "pnpm --filter @eye-note/frontend watch",
        "watch:extension": "pnpm --filter @eye-note/frontend watch:extension",
        "watch:ide": "pnpm --filter @eye-note/frontend watch:ide",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
    },
    "packageManager": "pnpm@8.15.1",
    "devDependencies": {
        "prettier": "^3.2.5"
    }
}
